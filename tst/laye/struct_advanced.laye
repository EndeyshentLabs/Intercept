// +     store i64 $tmp_main_14 into $tmp_main_8
// +     (?<tmp_main_15>$t) = load i64 from $tmp_main_8
// +     return i64 $tmp_main_15
int main()
{
    vec2i foo;
    foo.x = 34;
    foo.y = 330;
    vec2i bar;
    bar.x = 35;
    bar.y = 90;
    vec2i baz = vec2i_add(&foo, &bar);
    int qux = (baz.x * 10) + baz.y;
    return qux;
}
