// R %lcc --ir %s
// d b  [a-zA-Z0-9\.]+
// d t  %[0-9]+
// d f  _L[a-zA-Z0-9_]+
// d id [a-zA-Z0-9_]+

// * struct (?<vec2i>$id) { i64, i64 }
struct vec2i
{
    int x;
    int y;
}

// * $f : internal layecc @$vec2i(ptr (?<tmp_add_0>$t), ptr (?<tmp_add_1>$t)):
