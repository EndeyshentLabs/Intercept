// +     store ptr $tmp_add_1 into $tmp_add_3
// +     (?<tmp_add_4>$t) = alloca @$vec2i
// +     (?<tmp_add_5>$t) = gmp @$vec2i from $tmp_add_4 at i64 0
// +     (?<tmp_add_6>$t) = gmp @$vec2i from $tmp_add_2 at i64 0
// +     (?<tmp_add_7>$t) = load i64 from $tmp_add_6
// +     (?<tmp_add_8>$t) = gmp @$vec2i from $tmp_add_3 at i64 0
// +     (?<tmp_add_9>$t) = load i64 from $tmp_add_8
// +     (?<tmp_add_10>$t) = add i64 $tmp_add_7, $tmp_add_9
// +     store i64 $tmp_add_10 into $tmp_add_5
// +     (?<tmp_add_11>$t) = gmp @$vec2i from $tmp_add_4 at i64 1
// +     (?<tmp_add_12>$t) = gmp @$vec2i from $tmp_add_2 at i64 1
// +     (?<tmp_add_13>$t) = load i64 from $tmp_add_12
// +     (?<tmp_add_14>$t) = gmp @$vec2i from $tmp_add_3 at i64 1
// +     (?<tmp_add_15>$t) = load i64 from $tmp_add_14
// +     (?<tmp_add_16>$t) = add i64 $tmp_add_13, $tmp_add_15
// +     store i64 $tmp_add_16 into $tmp_add_11
// +     (?<tmp_add_17>$t) = load @$vec2i from $tmp_add_4
// +     return @$vec2i $tmp_add_17
vec2i vec2i_add(vec2i& a, vec2i& b)
{
    vec2i result;
    result.x = a.x + b.x;
    result.y = a.y + b.y;
    return result;
}

// * main : i64():
// +   bb0:
